{
  "dashboard": {
    "title": "MCP Tool Catalog - Genetic Algorithm Selection",
    "tags": [
      "neural-hive-mind",
      "mcp",
      "genetic-algorithm",
      "phase2"
    ],
    "timezone": "browser",
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "rows": [
      {
        "title": "Overview",
        "panels": [
          {
            "title": "Total Tool Selections",
            "type": "stat",
            "targets": [
              {
                "expr": "sum(mcp_tool_selections_total)"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "unit": "short",
                "thresholds": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "value": 0,
                      "color": "green"
                    },
                    {
                      "value": 100,
                      "color": "yellow"
                    },
                    {
                      "value": 1000,
                      "color": "red"
                    }
                  ]
                }
              }
            }
          },
          {
            "title": "Active Selections",
            "type": "gauge",
            "targets": [
              {
                "expr": "mcp_active_tool_selections"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "max": 10,
                "thresholds": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "value": 0,
                      "color": "green"
                    },
                    {
                      "value": 5,
                      "color": "yellow"
                    },
                    {
                      "value": 8,
                      "color": "red"
                    }
                  ]
                }
              }
            }
          },
          {
            "title": "Cache Hit Rate",
            "type": "gauge",
            "targets": [
              {
                "expr": "rate(mcp_cache_hits_total[5m]) / (rate(mcp_cache_hits_total[5m]) + rate(mcp_cache_misses_total[5m]))"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "unit": "percentunit",
                "max": 1,
                "thresholds": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "value": 0,
                      "color": "red"
                    },
                    {
                      "value": 0.5,
                      "color": "yellow"
                    },
                    {
                      "value": 0.8,
                      "color": "green"
                    }
                  ]
                }
              }
            }
          },
          {
            "title": "Registered Tools by Category",
            "type": "stat",
            "targets": [
              {
                "expr": "sum(mcp_registered_tools_total) by (category)",
                "legendFormat": "{{category}}"
              }
            ]
          }
        ]
      },
      {
        "title": "Genetic Algorithm Performance",
        "panels": [
          {
            "title": "GA Duration (p50, p95, p99)",
            "type": "graph",
            "targets": [
              {
                "expr": "histogram_quantile(0.50, rate(mcp_genetic_algorithm_duration_seconds_bucket[5m]))",
                "legendFormat": "p50"
              },
              {
                "expr": "histogram_quantile(0.95, rate(mcp_genetic_algorithm_duration_seconds_bucket[5m]))",
                "legendFormat": "p95"
              },
              {
                "expr": "histogram_quantile(0.99, rate(mcp_genetic_algorithm_duration_seconds_bucket[5m]))",
                "legendFormat": "p99"
              }
            ],
            "yaxes": [
              {
                "format": "s"
              }
            ]
          },
          {
            "title": "Generations Evolved",
            "type": "graph",
            "targets": [
              {
                "expr": "mcp_genetic_algorithm_generations"
              }
            ],
            "yaxes": [
              {
                "format": "short"
              }
            ]
          },
          {
            "title": "Fitness Score Distribution",
            "type": "heatmap",
            "targets": [
              {
                "expr": "rate(mcp_fitness_score_bucket[5m])",
                "format": "heatmap"
              }
            ]
          },
          {
            "title": "Selection Method Breakdown",
            "type": "piechart",
            "targets": [
              {
                "expr": "sum(mcp_tool_selections_total) by (selection_method)"
              }
            ]
          }
        ]
      },
      {
        "title": "Tool Execution",
        "panels": [
          {
            "title": "Tool Executions by Category",
            "type": "graph",
            "targets": [
              {
                "expr": "sum(rate(mcp_tool_executions_total[5m])) by (category)",
                "legendFormat": "{{category}}"
              }
            ]
          },
          {
            "title": "Tool Execution Success Rate",
            "type": "graph",
            "targets": [
              {
                "expr": "sum(rate(mcp_tool_executions_total{status=\"success\"}[5m])) / sum(rate(mcp_tool_executions_total[5m]))",
                "legendFormat": "Success Rate"
              }
            ],
            "yaxes": [
              {
                "format": "percentunit",
                "max": 1
              }
            ]
          },
          {
            "title": "Top 10 Tools by Usage",
            "type": "bargauge",
            "targets": [
              {
                "expr": "topk(10, sum(mcp_tool_executions_total) by (tool_id))"
              }
            ]
          }
        ]
      },
      {
        "title": "System Health",
        "panels": [
          {
            "title": "Pod Status",
            "type": "stat",
            "targets": [
              {
                "expr": "count(kube_pod_info{pod=~\"mcp-tool-catalog.*\", phase=\"Running\"})"
              }
            ]
          },
          {
            "title": "CPU Usage",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(container_cpu_usage_seconds_total{pod=~\"mcp-tool-catalog.*\"}[5m])"
              }
            ],
            "yaxes": [
              {
                "format": "percentunit"
              }
            ]
          },
          {
            "title": "Memory Usage",
            "type": "graph",
            "targets": [
              {
                "expr": "container_memory_usage_bytes{pod=~\"mcp-tool-catalog.*\"} / container_spec_memory_limit_bytes"
              }
            ],
            "yaxes": [
              {
                "format": "percentunit"
              }
            ]
          },
          {
            "title": "Request Rate",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(mcp_tool_selections_total[5m])"
              }
            ],
            "yaxes": [
              {
                "format": "reqps"
              }
            ]
          }
        ]
      },
      {
        "title": "Tool Catalog Completeness",
        "panels": [
          {
            "title": "Tools by Category",
            "type": "barchart",
            "targets": [
              {
                "expr": "sum(mcp_registered_tools_total) by (category)"
              }
            ]
          },
          {
            "title": "Tool Health Status",
            "type": "piechart",
            "targets": [
              {
                "expr": "mcp_healthy_tools_total"
              },
              {
                "expr": "mcp_registered_tools_total - mcp_healthy_tools_total"
              }
            ]
          },
          {
            "title": "Tool Reputation Distribution",
            "type": "heatmap",
            "targets": [
              {
                "expr": "avg(mcp_tool_reputation_score) by (category)"
              }
            ]
          },
          {
            "title": "Most Used Tools (Top 20)",
            "type": "table",
            "targets": [
              {
                "expr": "topk(20, sum(mcp_tool_executions_total) by (tool_id, tool_name))"
              }
            ]
          }
        ]
      },
      {
        "title": "Integration Metrics",
        "panels": [
          {
            "title": "Code Forge Integration Rate",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(mcp_tool_selections_total{source=\"code-forge\"}[5m])"
              }
            ]
          },
          {
            "title": "Feedback Loop Latency",
            "type": "graph",
            "targets": [
              {
                "expr": "histogram_quantile(0.95, rate(mcp_feedback_latency_seconds_bucket[5m]))"
              }
            ]
          },
          {
            "title": "Selection Cache Efficiency",
            "type": "stat",
            "thresholds": [
              {
                "value": 70,
                "color": "green"
              }
            ],
            "targets": [
              {
                "expr": "(sum(mcp_tool_selection_cache_hits_total) / sum(mcp_tool_selection_requests_total)) * 100"
              }
            ]
          },
          {
            "title": "Adapter Execution Time by Type",
            "type": "graph",
            "targets": [
              {
                "expr": "rate(mcp_tool_execution_duration_seconds_bucket[5m]) by (adapter_type)"
              }
            ]
          }
        ]
      },
      {
        "title": "MCP Server Integration (Stdio Transport)",
        "panels": [
          {
            "title": "Stdio Request Rate",
            "type": "graph",
            "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
            "targets": [
              {
                "expr": "rate(mcp_stdio_requests_total[5m])",
                "legendFormat": "{{method}} - {{status}}"
              }
            ],
            "yaxes": [
              {
                "format": "reqps"
              }
            ]
          },
          {
            "title": "Stdio Latency (p50, p95, p99)",
            "type": "graph",
            "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
            "targets": [
              {
                "expr": "histogram_quantile(0.50, rate(mcp_stdio_request_duration_seconds_bucket[5m]))",
                "legendFormat": "p50"
              },
              {
                "expr": "histogram_quantile(0.95, rate(mcp_stdio_request_duration_seconds_bucket[5m]))",
                "legendFormat": "p95"
              },
              {
                "expr": "histogram_quantile(0.99, rate(mcp_stdio_request_duration_seconds_bucket[5m]))",
                "legendFormat": "p99"
              }
            ],
            "yaxes": [
              {
                "format": "s"
              }
            ]
          },
          {
            "title": "Stdio Success Rate",
            "type": "gauge",
            "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
            "targets": [
              {
                "expr": "sum(rate(mcp_stdio_requests_total{status=\"success\"}[5m])) / sum(rate(mcp_stdio_requests_total[5m]))"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "unit": "percentunit",
                "max": 1,
                "thresholds": {
                  "mode": "absolute",
                  "steps": [
                    {"value": 0, "color": "red"},
                    {"value": 0.9, "color": "yellow"},
                    {"value": 0.98, "color": "green"}
                  ]
                }
              }
            }
          },
          {
            "title": "Subprocess Restarts",
            "type": "graph",
            "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
            "targets": [
              {
                "expr": "rate(mcp_stdio_subprocess_restarts_total[5m])",
                "legendFormat": "restarts/s"
              }
            ],
            "yaxes": [
              {
                "format": "short"
              }
            ]
          },
          {
            "title": "Stdio Requests by Method",
            "type": "piechart",
            "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
            "targets": [
              {
                "expr": "sum(mcp_stdio_requests_total) by (method)"
              }
            ]
          }
        ]
      }
    ]
  }
}