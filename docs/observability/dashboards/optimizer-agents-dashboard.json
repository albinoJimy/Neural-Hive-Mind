{
  "dashboard": {
    "title": "Optimizer Agents - Performance Dashboard",
    "tags": ["optimizer-agents", "ml", "reinforcement-learning", "experiments"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "Total Optimizations Applied",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 1},
        "targets": [
          {
            "expr": "sum(optimizations_applied_total)",
            "legendFormat": "Total"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "Optimization Success Rate",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 1},
        "targets": [
          {
            "expr": "avg(optimization_success_rate)",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 0.8, "color": "yellow"},
                {"value": 0.9, "color": "green"}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 4,
        "title": "Average Improvement",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 1},
        "targets": [
          {
            "expr": "avg(average_improvement_percentage)",
            "legendFormat": "Avg Improvement"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "yellow"},
                {"value": 0.05, "color": "green"}
              ]
            },
            "unit": "percentunit"
          }
        }
      },
      {
        "id": 5,
        "title": "Active Experiments",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 1},
        "targets": [
          {
            "expr": "experiments_active",
            "legendFormat": "Active"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "blue"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 10,
        "title": "Reinforcement Learning",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 5},
        "collapsed": false
      },
      {
        "id": 11,
        "title": "Q-Table Size",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "q_table_size",
            "legendFormat": "States"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "States"},
          {"format": "short"}
        ]
      },
      {
        "id": 12,
        "title": "Epsilon Value (Exploration Rate)",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "epsilon_value",
            "legendFormat": "Epsilon"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Epsilon", "min": 0, "max": 1},
          {"format": "short"}
        ]
      },
      {
        "id": 13,
        "title": "Hypotheses Generated by Type",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(hypotheses_generated_total[5m])",
            "legendFormat": "{{type}}"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Hypotheses/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 14,
        "title": "Reward Distribution",
        "type": "heatmap",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "sum(rate(reward_distribution_bucket[5m])) by (le)",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ]
      },
      {
        "id": 20,
        "title": "Experiments",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 18},
        "collapsed": false
      },
      {
        "id": 21,
        "title": "Experiments Submitted",
        "type": "graph",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 19},
        "targets": [
          {
            "expr": "rate(experiments_submitted_total[5m])",
            "legendFormat": "{{type}}"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Experiments/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 22,
        "title": "Experiment Duration",
        "type": "graph",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 19},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(experiment_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(experiment_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 23,
        "title": "Experiment Success vs Failure",
        "type": "piechart",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 19},
        "targets": [
          {
            "expr": "sum(experiments_succeeded_total)",
            "legendFormat": "Succeeded"
          },
          {
            "expr": "sum(experiments_failed_total)",
            "legendFormat": "Failed"
          },
          {
            "expr": "sum(experiments_below_criteria_total)",
            "legendFormat": "Below Criteria"
          }
        ]
      },
      {
        "id": 30,
        "title": "Optimizations",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 25},
        "collapsed": false
      },
      {
        "id": 31,
        "title": "Optimizations by Type",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 26},
        "targets": [
          {
            "expr": "rate(optimizations_applied_total[5m])",
            "legendFormat": "{{type}}"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Optimizations/sec"},
          {"format": "short"}
        ],
        "stack": true
      },
      {
        "id": 32,
        "title": "Weight Adjustments Magnitude",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 26},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(weight_adjustment_magnitude_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(weight_adjustment_magnitude_bucket[5m]))",
            "legendFormat": "P50"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Magnitude"},
          {"format": "short"}
        ]
      },
      {
        "id": 33,
        "title": "Rollbacks",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "sum(rollbacks_total)",
            "legendFormat": "Total Rollbacks"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 40,
        "title": "Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 36},
        "collapsed": false
      },
      {
        "id": 41,
        "title": "Optimization Processing Duration",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 37},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(optimization_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "P99"
          },
          {
            "expr": "histogram_quantile(0.95, rate(optimization_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(optimization_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 42,
        "title": "Kafka Consumer Lag",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 37},
        "targets": [
          {
            "expr": "kafka_consumer_lag{service='optimizer-agents'}",
            "legendFormat": "{{topic}}"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Messages"},
          {"format": "short"}
        ]
      },
      {
        "id": 50,
        "title": "Resources",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 43},
        "collapsed": false
      },
      {
        "id": 51,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 44},
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{pod=~'optimizer-agents.*'}[5m])",
            "legendFormat": "{{pod}}"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "CPU"},
          {"format": "short"}
        ]
      },
      {
        "id": 52,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 44},
        "targets": [
          {
            "expr": "container_memory_working_set_bytes{pod=~'optimizer-agents.*'} / 1024 / 1024",
            "legendFormat": "{{pod}}"
          }
        ],
        "yaxes": [
          {"format": "decmbytes", "label": "Memory"},
          {"format": "short"}
        ]
      }
    ]
  }
}
