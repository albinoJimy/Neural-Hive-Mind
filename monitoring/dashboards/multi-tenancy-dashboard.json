{
  "dashboard": {
    "title": "Neural Hive - Multi-Tenancy",
    "tags": ["neural-hive", "multi-tenancy", "specialists"],
    "timezone": "utc",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Avaliações por Tenant (Total)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(neural_hive_tenant_evaluations_total[5m])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "calcs": ["last", "mean", "max"]
          },
          "tooltip": {
            "mode": "multi"
          }
        }
      },
      {
        "id": 2,
        "title": "Top 10 Tenants por Volume",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "topk(10, sum by (tenant_id) (rate(neural_hive_tenant_evaluations_total[5m])))",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 3,
        "title": "Latência P95 por Tenant",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (tenant_id, le) (rate(neural_hive_tenant_evaluation_duration_seconds_bucket[5m])))",
            "legendFormat": "{{tenant_id}} (P95)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Latência P50 vs P99 por Tenant",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum by (tenant_id, le) (rate(neural_hive_tenant_evaluation_duration_seconds_bucket[5m])))",
            "legendFormat": "{{tenant_id}} (P50)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, sum by (tenant_id, le) (rate(neural_hive_tenant_evaluation_duration_seconds_bucket[5m])))",
            "legendFormat": "{{tenant_id}} (P99)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 5,
        "title": "Taxa de Cache Hit por Tenant",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(neural_hive_tenant_cache_hits_total[5m])) by (tenant_id) / (sum(rate(neural_hive_tenant_cache_hits_total[5m])) by (tenant_id) + sum(rate(neural_hive_tenant_cache_misses_total[5m])) by (tenant_id))",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.8, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Cache Hits vs Misses (Total)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(neural_hive_tenant_cache_hits_total[5m]))",
            "refId": "A"
          },
          {
            "expr": "sum(rate(neural_hive_tenant_cache_misses_total[5m]))",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Hits"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Misses"},
              "properties": [
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal"
        }
      },
      {
        "id": 7,
        "title": "Rate Limit Hits por Tenant",
        "type": "timeseries",
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(envoy_cluster_upstream_rq_429[5m])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "bars"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "orange"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Erros por Tenant e Tipo",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(neural_hive_tenant_errors_total[5m])) by (tenant_id, error_type)",
            "legendFormat": "{{tenant_id}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Taxa de Erro por Tenant",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(neural_hive_tenant_errors_total[5m])) by (tenant_id) / sum(rate(neural_hive_tenant_evaluations_total[5m])) by (tenant_id)",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.01, "color": "yellow"},
                {"value": 0.05, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Distribuição de Tenants Ativos",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "count(sum by (tenant_id) (rate(neural_hive_tenant_evaluations_total[5m]) > 0))",
            "legendFormat": "Ativos",
            "refId": "A"
          },
          {
            "expr": "count(sum by (tenant_id) (rate(neural_hive_tenant_evaluations_total[5m]) == 0))",
            "legendFormat": "Inativos",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          },
          "pieType": "donut"
        }
      },
      {
        "id": 11,
        "title": "Cardinality de Tenants (Known vs Other)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 32},
        "targets": [
          {
            "expr": "count(count by (tenant_id) (neural_hive_tenant_evaluations_total{tenant_id!=\"other\"}))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 40, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {
            "titleSize": 14,
            "valueSize": 48
          }
        }
      },
      {
        "id": 12,
        "title": "Requests Agregados em 'other'",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(neural_hive_tenant_evaluations_total{tenant_id=\"other\"}[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background"
        }
      },
      {
        "id": 13,
        "title": "Tenant com Maior Latência P95",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "topk(1, histogram_quantile(0.95, sum by (tenant_id, le) (rate(neural_hive_tenant_evaluation_duration_seconds_bucket[5m]))))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 14,
        "title": "Tenant com Menor Cache Hit Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 36},
        "targets": [
          {
            "expr": "bottomk(1, sum(rate(neural_hive_tenant_cache_hits_total[5m])) by (tenant_id) / (sum(rate(neural_hive_tenant_cache_hits_total[5m])) by (tenant_id) + sum(rate(neural_hive_tenant_cache_misses_total[5m])) by (tenant_id)))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.8, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 15,
        "title": "Tenant com Maior Taxa de Erro",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 36},
        "targets": [
          {
            "expr": "topk(1, sum(rate(neural_hive_tenant_errors_total[5m])) by (tenant_id) / sum(rate(neural_hive_tenant_evaluations_total[5m])) by (tenant_id))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.01, "color": "yellow"},
                {"value": 0.05, "color": "red"}
              ]
            }
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=~\".*Tenant.*\",alertstate=\"firing\"}",
          "iconColor": "red",
          "name": "Tenant Alerts",
          "tagKeys": "tenant_id,alertname",
          "textFormat": "{{alertname}} - {{tenant_id}}",
          "titleFormat": "Alert"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(neural_hive_tenant_evaluations_total, tenant_id)",
          "hide": 0,
          "includeAll": true,
          "label": "Tenant",
          "multi": true,
          "name": "tenant_id",
          "query": "label_values(neural_hive_tenant_evaluations_total, tenant_id)",
          "refresh": 1,
          "regex": "",
          "sort": 1,
          "type": "query"
        }
      ]
    }
  }
}
