{
  "dashboard": {
    "title": "Neural Hive - Business Metrics",
    "tags": ["neural-hive", "business-metrics", "consensus"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "templating": {
      "list": [
        {
          "name": "specialist_type",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(neural_hive_business_consensus_agreement_rate, specialist_type)",
          "multi": true,
          "includeAll": true,
          "allValue": ".*",
          "refresh": 1,
          "sort": 1
        },
        {
          "name": "environment",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(neural_hive_business_consensus_agreement_rate, environment)",
          "multi": false,
          "includeAll": false,
          "refresh": 1,
          "sort": 1
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Taxa de Concordância com Consenso",
        "type": "gauge",
        "targets": [
          {
            "expr": "neural_hive_business_consensus_agreement_rate{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "{{specialist_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "orange"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Precision Score Médio",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(neural_hive_business_precision_score{specialist_type=~\"$specialist_type\"})",
            "legendFormat": "Avg Precision"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "orange"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Recall Score Médio",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(neural_hive_business_recall_score{specialist_type=~\"$specialist_type\"})",
            "legendFormat": "Avg Recall"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "orange"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "F1 Score Médio",
        "type": "gauge",
        "targets": [
          {
            "expr": "avg(neural_hive_business_f1_score{specialist_type=~\"$specialist_type\"})",
            "legendFormat": "Avg F1"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "orange"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Confusion Matrix - True Positives",
        "type": "stat",
        "targets": [
          {
            "expr": "neural_hive_business_true_positives_total{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "{{specialist_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Confusion Matrix - True Negatives",
        "type": "stat",
        "targets": [
          {
            "expr": "neural_hive_business_true_negatives_total{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "{{specialist_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8}
      },
      {
        "id": 7,
        "title": "Confusion Matrix - False Positives",
        "type": "stat",
        "targets": [
          {
            "expr": "neural_hive_business_false_positives_total{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "{{specialist_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8}
      },
      {
        "id": 8,
        "title": "Confusion Matrix - False Negatives",
        "type": "stat",
        "targets": [
          {
            "expr": "neural_hive_business_false_negatives_total{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "{{specialist_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8}
      },
      {
        "id": 9,
        "title": "Taxa de Concordância por Especialista (Linha do Tempo)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "neural_hive_business_consensus_agreement_rate{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "{{specialist_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 10,
        "title": "Taxas de Falso Positivo e Falso Negativo",
        "type": "timeseries",
        "targets": [
          {
            "expr": "neural_hive_business_false_positive_rate{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "FP Rate - {{specialist_type}}"
          },
          {
            "expr": "neural_hive_business_false_negative_rate{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "FN Rate - {{specialist_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12}
      },
      {
        "id": 11,
        "title": "Precision, Recall e F1 Score por Especialista",
        "type": "barchart",
        "targets": [
          {
            "expr": "neural_hive_business_precision_score{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "Precision - {{specialist_type}}"
          },
          {
            "expr": "neural_hive_business_recall_score{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "Recall - {{specialist_type}}"
          },
          {
            "expr": "neural_hive_business_f1_score{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "F1 - {{specialist_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 12,
        "title": "Valor de Negócio Gerado (Total)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(neural_hive_business_value_generated_total{specialist_type=~\"$specialist_type\"})",
            "legendFormat": "Total Business Value"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "text"},
                {"value": 100, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 20}
      },
      {
        "id": 13,
        "title": "Taxa de Valor de Negócio Gerado",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(neural_hive_business_value_generated_total{specialist_type=~\"$specialist_type\"}[5m])",
            "legendFormat": "{{specialist_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 20}
      },
      {
        "id": 14,
        "title": "Anomalias Detectadas (Últimas 24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(neural_hive_anomaly_detected_total{specialist_type=~\"$specialist_type\"}[24h]))",
            "legendFormat": "Total Anomalies"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "orange"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24}
      },
      {
        "id": 15,
        "title": "Timeline de Anomalias por Severidade",
        "type": "timeseries",
        "targets": [
          {
            "expr": "increase(neural_hive_anomaly_detected_total{specialist_type=~\"$specialist_type\", severity=\"warning\"}[5m])",
            "legendFormat": "Warning - {{specialist_type}}"
          },
          {
            "expr": "increase(neural_hive_anomaly_detected_total{specialist_type=~\"$specialist_type\", severity=\"critical\"}[5m])",
            "legendFormat": "Critical - {{specialist_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "drawStyle": "bars"
            }
          }
        },
        "gridPos": {"h": 8, "w": 18, "x": 6, "y": 24}
      },
      {
        "id": 16,
        "title": "Última Atualização de Métricas",
        "type": "stat",
        "targets": [
          {
            "expr": "time() - neural_hive_business_metrics_last_update_timestamp{specialist_type=~\"$specialist_type\"}",
            "legendFormat": "{{specialist_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 3600, "color": "yellow"},
                {"value": 7200, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 17,
        "title": "Health Check - Business Metrics Collector",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"business-metrics-collector\"}",
            "legendFormat": "Collector Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "UP", "color": "green"},
              {"type": "value", "value": "0", "text": "DOWN", "color": "red"}
            ],
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 32}
      }
    ],
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=~\".*BusinessMetrics.*|.*Anomaly.*\"}",
          "iconColor": "red",
          "name": "Business Metrics Alerts",
          "titleFormat": "{{alertname}}"
        }
      ]
    }
  }
}
