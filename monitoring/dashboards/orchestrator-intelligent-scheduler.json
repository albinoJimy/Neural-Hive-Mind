{
  "dashboard": {
    "title": "Orchestrator - Intelligent Scheduler",
    "uid": "orchestrator-intelligent-scheduler",
    "tags": ["orchestrator", "scheduler", "phase2"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Allocation Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(orchestration_scheduler_allocations_total{status=\"success\"}[5m]) / rate(orchestration_scheduler_allocations_total[5m]) * 100",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "graphMode": "area"
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Fallback Usage Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(orchestration_scheduler_allocations_total{fallback=\"true\"}[5m]) / rate(orchestration_scheduler_allocations_total[5m]) * 100",
            "legendFormat": "Fallback Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Average Allocation Latency (P95)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(orchestration_scheduler_allocation_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Latency"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.2, "color": "yellow"},
                {"value": 0.5, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "graphMode": "area"
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Workers Discovered (Avg)",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(orchestration_scheduler_workers_discovered)",
            "legendFormat": "Avg Workers"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Priority Scores by Risk Band",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(orchestration_scheduler_priority_score_bucket{risk_band=~\"$risk_band\"}[5m]))",
            "legendFormat": "{{risk_band}}"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Priority Score"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "Priority Score Heatmap",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum(rate(orchestration_scheduler_priority_score_bucket[5m])) by (le, risk_band)",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 7,
        "title": "Discovery Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "1 - (rate(orchestration_scheduler_discovery_failures_total[5m]) / rate(orchestration_scheduler_allocations_total[5m]))",
            "legendFormat": "Success Rate"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Success Rate"}
        ],
        "fill": 2,
        "linewidth": 2,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "Discovery Failures by Type",
        "type": "bargauge",
        "targets": [
          {
            "expr": "sum(rate(orchestration_scheduler_discovery_failures_total[5m])) by (error_type)",
            "legendFormat": "{{error_type}}"
          }
        ],
        "options": {
          "orientation": "horizontal"
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 12}
      },
      {
        "id": 9,
        "title": "Workers Discovered Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(orchestration_scheduler_workers_discovered_bucket[5m]))",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(orchestration_scheduler_workers_discovered_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(orchestration_scheduler_workers_discovered_bucket[5m]))",
            "legendFormat": "P99"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Workers"}
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 12}
      },
      {
        "id": 10,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(orchestration_scheduler_cache_hits_total[5m]) / (rate(orchestration_scheduler_cache_hits_total[5m]) + rate(orchestration_scheduler_allocations_total[5m]))",
            "legendFormat": "Hit Rate"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Hit Rate"}
        ],
        "thresholds": [
          {"value": 0.5, "colorMode": "critical"},
          {"value": 0.7, "colorMode": "custom"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 11,
        "title": "Cache Hits vs Misses",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(orchestration_scheduler_cache_hits_total[5m]))",
            "legendFormat": "Hits"
          },
          {
            "expr": "sum(rate(orchestration_scheduler_allocations_total[5m])) - sum(rate(orchestration_scheduler_cache_hits_total[5m]))",
            "legendFormat": "Misses"
          }
        ],
        "stack": true,
        "yaxes": [
          {"format": "reqps", "label": "Rate"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      },
      {
        "id": 12,
        "title": "Rejection Reasons",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(rate(orchestration_scheduler_rejections_total[5m])) by (reason)",
            "legendFormat": "{{reason}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28}
      },
      {
        "id": 13,
        "title": "Rejection Rate Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(orchestration_scheduler_rejections_total[5m])) by (reason)",
            "legendFormat": "{{reason}}"
          }
        ],
        "stack": true,
        "yaxes": [
          {"format": "reqps", "label": "Rejections/s"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28}
      },
      {
        "id": 14,
        "title": "Allocation Latency Percentiles",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(orchestration_scheduler_allocation_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(orchestration_scheduler_allocation_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(orchestration_scheduler_allocation_duration_seconds_bucket[5m]))",
            "legendFormat": "P99"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36}
      },
      {
        "id": 15,
        "title": "Allocation Latency Heatmap",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum(rate(orchestration_scheduler_allocation_duration_seconds_bucket[5m])) by (le)",
            "format": "heatmap"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36}
      },
      {
        "id": 16,
        "title": "Tickets with ML Predictions",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(orchestration_scheduler_allocations_total{has_predictions=\"true\"})",
            "legendFormat": "With Predictions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 4, "w": 12, "x": 0, "y": 44}
      },
      {
        "id": 17,
        "title": "Priority Boosts from ML",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(orchestration_scheduler_priority_score{boosted=\"true\"}[5m])",
            "legendFormat": "Boosted"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Count"}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 44}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "namespace",
          "type": "query",
          "query": "label_values(orchestration_scheduler_allocations_total, namespace)",
          "multi": true,
          "includeAll": true
        },
        {
          "name": "risk_band",
          "type": "query",
          "query": "label_values(orchestration_scheduler_priority_score, risk_band)",
          "multi": true,
          "includeAll": true
        },
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "-- Grafana --",
          "iconColor": "blue",
          "tags": ["deployment"]
        },
        {
          "name": "Service Registry Outages",
          "datasource": "-- Grafana --",
          "iconColor": "red",
          "tags": ["service-registry", "outage"]
        }
      ]
    },
    "links": [
      {
        "title": "Flow C Orchestration",
        "url": "/d/fluxo-c-orquestracao",
        "tooltip": "View orchestration overview"
      },
      {
        "title": "Service Registry",
        "url": "/d/service-registry",
        "tooltip": "View Service Registry metrics"
      },
      {
        "title": "Documentation",
        "url": "/docs/INTELLIGENT_SCHEDULER_INTEGRATION.md",
        "tooltip": "Scheduler integration guide"
      }
    ]
  }
}
