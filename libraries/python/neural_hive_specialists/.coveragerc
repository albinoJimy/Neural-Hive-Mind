[run]
source = neural_hive_specialists
omit =
    */tests/*
    */proto_gen/*
    */__pycache__/*
    */setup.py
branch = True
parallel = True

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
title = Neural Hive Specialists Coverage Report

[xml]
output = coverage.xml
